<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<ScrollPane fx:id="MainSP" fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userinterface.admin.topAdmin.TopAdminController">
   <content>
      <BorderPane fx:id="MainBP" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="475.0" prefWidth="600.0">
         <top>
            <HBox alignment="BOTTOM_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="60.0" prefWidth="600.0" spacing="10.0">
               <children>
                  <ComboBox fx:id="UserCB" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onAction="#SetCBOnAction" prefHeight="38.0" prefWidth="150.0" />
                  <Label fx:id="FileLABEL" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="23.0" prefWidth="328.0" text="File:" HBox.hgrow="ALWAYS" />
                  <Label fx:id="YazLABEL" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="100.0" prefHeight="17.0" prefWidth="77.0" text="Current Yaz:" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
         </top>
         <center>
            <fx:include fx:id="CenterAdmin" source="../centerAdmin/CenterAdmin.fxml" />
         </center>
      </BorderPane>
   </content>
</ScrollPane>
