         <children>
            <Button fx:id="IncreaseYazBUTTON" minWidth="50.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="111.0" text="Increase Yaz" />
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
